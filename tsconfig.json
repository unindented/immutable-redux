{
  "compilerOptions": {
    "outDir": "./lib/",
    "lib": ["es5", "es2015.symbol"],
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es5",
    "declaration": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "types": [
      "jest",
      "jasmine-immutable-matchers",
      "node"
