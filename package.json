{
  "name": "immutable-redux",
  "description": "Provides utilities for dealing with Immutable.js data structures in Redux.",
  "version": "0.2.1",
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/unindented/immutable-redux.git"
  },
  "scripts": {
    "clean": "rimraf coverage dist lib",
    "lint": "eslint .",
    "test": "karma start --single-run",
    "test:watch": "karma start --no-single-run",
    "build:lib": "babel src --out-dir lib",
    "build:dev": "webpack --config webpack.config.development.js",
    "build:prod": "webpack --config webpack.config.production.js",
    "build": "npm run build:lib && npm run build:dev && npm run build:prod",
    "prepublish": "npm run clean && npm run build"
  },
  "peerDependencies": {
    "immutable": "^3.7.5"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-1": "^6.16.0",
    "babel-runtime": "^6.18.0",
    "coveralls": "^2.11.14",
    "eslint": "^3.9.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-standard": "^2.0.1",
    "immutable": "^3.8.1",
    "isparta-loader": "^2.0.0",
    "jasmine-core": "^2.5.2",
    "jasmine-immutable-matchers": "^1.0.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-opera-launcher": "^1.0.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-safari-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "node-libs-browser": "^2.0.0",
    "phantomjs-prebuilt": "^2.1.13",
    "redux": "^3.6.0",
    "rimraf": "^2.5.4",
    "webpack": "^1.13.3"
  }
}
