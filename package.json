{
  "name": "immutable-redux",
  "description": "Provides utilities for dealing with Immutable.js data structures in Redux.",
  "version": "0.0.4",
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/unindented/immutable-redux.git"
  },
  "scripts": {
    "clean": "rimraf coverage dist lib",
    "lint": "eslint .",
    "test": "karma start --single-run",
    "test:watch": "karma start --no-single-run",
    "build:lib": "babel src --out-dir lib",
    "build:dev": "webpack --config webpack.config.development.js",
    "build:prod": "webpack --config webpack.config.production.js",
    "build": "npm run build:lib && npm run build:dev && npm run build:prod",
    "prepublish": "npm run clean && npm run build"
  },
  "peerDependencies": {
    "immutable": "^3.7.5"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^6.1.18",
    "babel-eslint": "^4.1.3",
    "babel-runtime": "^5.8.25",
    "coveralls": "^2.11.4",
    "eslint": "^1.6.0",
    "eslint-config-standard": "^4.4.0",
    "eslint-plugin-standard": "^1.3.0",
    "immutable": "^3.7.5",
    "jasmine-core": "^2.3.4",
    "jasmine-immutable-matchers": "^0.1.0",
    "node-libs-browser": "^0.5.3",
    "phantomjs": "^1.9.18",
    "redux": "^3.0.2",
    "rimraf": "^2.3.4",
    "webpack": "^1.12.2",
    "babel-loader": "^5.3.2",
    "istanbul-instrumenter-loader": "^0.1.3",
    "karma": "^0.13.10",
    "karma-coverage": "^0.5.1",
    "karma-jasmine": "^0.3.6",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-chrome-launcher": "^0.2.1",
    "karma-firefox-launcher": "^0.1.6",
    "karma-opera-launcher": "^0.3.0",
    "karma-safari-launcher": "^0.1.1",
    "karma-ie-launcher": "^0.2.0"
  }
}
