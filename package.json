{
  "name": "immutable-redux",
  "description": "Provides utilities for dealing with Immutable.js data structures in Redux.",
  "version": "0.1.2",
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/unindented/immutable-redux.git"
  },
  "scripts": {
    "clean": "rimraf coverage dist lib",
    "lint": "eslint .",
    "test": "karma start --single-run",
    "test:watch": "karma start --no-single-run",
    "build:lib": "babel src --out-dir lib",
    "build:dev": "webpack --config webpack.config.development.js",
    "build:prod": "webpack --config webpack.config.production.js",
    "build": "npm run build:lib && npm run build:dev && npm run build:prod",
    "prepublish": "npm run clean && npm run build"
  },
  "peerDependencies": {
    "immutable": "^3.7.5"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "coveralls": "^2.11.4",
    "eslint": "^1.6.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-standard": "^1.3.0",
    "immutable": "^3.7.5",
    "istanbul-instrumenter-loader": "^0.1.3",
    "jasmine-core": "^2.4.1",
    "jasmine-immutable-matchers": "^1.0.1",
    "karma": "^0.13.16",
    "karma-chrome-launcher": "^0.2.1",
    "karma-coverage": "^0.5.1",
    "karma-firefox-launcher": "^0.1.6",
    "karma-ie-launcher": "^0.2.0",
    "karma-jasmine": "^0.3.6",
    "karma-opera-launcher": "^0.3.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-safari-launcher": "^0.1.1",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "node-libs-browser": "^1.0.0",
    "phantomjs-prebuilt": "^2.1.3",
    "redux": "^3.2.0",
    "rimraf": "^2.5.0",
    "webpack": "^1.12.9"
  }
}
