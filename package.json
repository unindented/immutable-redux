{
  "name": "immutable-redux",
  "description": "Provides utilities for dealing with Immutable.js data structures in Redux.",
  "version": "0.1.3",
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/unindented/immutable-redux.git"
  },
  "scripts": {
    "clean": "rimraf coverage dist lib",
    "lint": "eslint .",
    "test": "karma start --single-run",
    "test:watch": "karma start --no-single-run",
    "build:lib": "babel src --out-dir lib",
    "build:dev": "webpack --config webpack.config.development.js",
    "build:prod": "webpack --config webpack.config.production.js",
    "build": "npm run build:lib && npm run build:dev && npm run build:prod",
    "prepublish": "npm run clean && npm run build"
  },
  "peerDependencies": {
    "immutable": "^3.7.5"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.1",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.2",
    "babel-plugin-transform-runtime": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-runtime": "^6.5.0",
    "coveralls": "^2.11.6",
    "eslint": "^2.5.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.0.8",
    "eslint-plugin-standard": "^1.3.2",
    "immutable": "^3.7.6",
    "isparta-loader": "^2.0.0",
    "jasmine-core": "^2.4.1",
    "jasmine-immutable-matchers": "^1.0.1",
    "karma": "^1.0.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.0",
    "karma-opera-launcher": "^1.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "node-libs-browser": "^1.0.0",
    "phantomjs-prebuilt": "^2.1.4",
    "redux": "^3.3.1",
    "rimraf": "^2.5.1",
    "webpack": "^1.12.13"
  }
}
